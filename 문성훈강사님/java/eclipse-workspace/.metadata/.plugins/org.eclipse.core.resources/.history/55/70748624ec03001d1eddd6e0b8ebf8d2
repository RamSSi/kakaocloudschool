package lecture0715;




class Thread_instance implements Runnable {
	public synchronized void printName() {
		System.out.println();
	}
	
	@Override
	public void run() {
		printName();
	}
}

public class ThreadProblem {

	public static void main(String[] args) {
		Thread_instance t1 = new Thread_instance();
		Thread_instance t2 = new Thread_instance();
		
		t1.start();
		t2.start();
	}

}
